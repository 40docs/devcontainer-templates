{
  "name": "40docs DevContainer",
  "remoteUser": "vscode",
  "shutdownAction": "stopContainer",
  "hostRequirements": {
    "cpus": 4,
    "memory": "8gb",
    "gpu": "optional"
  },
  "forwardPorts": [3000, 5000, 8000, 8080, 9000],
  "portsAttributes": {
    "8000": {
      "label": "MkDocs Server",
      "onAutoForward": "notify"
    },
    "3000": {
      "label": "Development Server",
      "onAutoForward": "openBrowser"
    }
  },
  "customizations": {
    "vscode": {
      "settings": {
        "csharp.devkit.preferVsCodeFileSystemWatcher": true,
        "dotnetAcquisitionExtension.existingDotnetPath": [
          {
            "extensionId": "MS-CST-E.vscode-devskim",
            "path": "/usr/share/dotnet/dotnet"
          },
          {
            "extensionId": "ms-azuretools.vscode-azure-github-copilot",
            "path": "/usr/share/dotnet/dotnet"
          }
        ],
        "dotnetAcquisitionExtension.sharedExistingDotnetPath": "/usr/share/dotnet/dotnet",
        "files.eol": "\n",
        "mcp": {
          "servers": {
            "github": {
              "type": "http",
              "url": "https://api.githubcopilot.com/mcp/"
            },
            "playwright": {
              "args": [
                "@playwright/mcp@latest"
              ],
              "command": "npx"
            }
          }
        },
        "remote.containers.copyGitConfig": true,
        "terminal.integrated.defaultProfile.linux": "zsh",
        "terminal.integrated.profiles.linux": {
          "bash": {
            "path": "/bin/bash"
          },
          "pwsh": {
            "path": "/opt/microsoft/powershell/7/pwsh"
          },
          "tmux": {
            "args": [
              "new-session",
              "-A",
              "-s",
              "vscode:${workspaceFolder}"
            ],
            "path": "/usr/bin/tmux"
          },
          "zsh": {
            "path": "/usr/bin/zsh"
          }
        }
      }
    }
  },
